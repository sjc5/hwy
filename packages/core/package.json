{
  "name": "hwy",
  "version": "0.0.0-beta.69",
  "author": {
    "name": "Samuel J. Cook"
  },
  "license": "MIT",
  "description": "Simple, lightweight, and flexible web framework.",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsup-node index.ts --format esm --dts",
    "dev": "rm -rf dist && tsup-node index.ts --format esm --dts --watch",
    "publish-beta": "npm publish --tag beta",
    "publish-non-beta": "npm publish",
    "update-latest": "pnpm update --latest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "devalue": "^4.3.2"
  },
  "devDependencies": {
    "@hwy-js/build": "workspace:*",
    "@hwy-js/dev": "workspace:*",
    "@types/node": "^20.11.28",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "h3": "^1.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hwy-js/hwy",
    "directory": "packages/core"
  },
  "homepage": "https://hwy.dev",
  "bugs": "https://github.com/hwy-js/hwy/issues",
  "keywords": [
    "simple",
    "lightweight",
    "flexible",
    "framework",
    "metaframework",
    "web",
    "typescript",
    "unjs",
    "h3",
    "router",
    "nested",
    "routing",
    "jsx",
    "tsx",
    "nextjs",
    "remix",
    "react"
  ],
  "sideEffects": false
}
